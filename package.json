{
	"name": "sync-everything",
	"displayName": "Sync Everything",
	"description": "Sync your VS Code/Cursor settings, extensions, and keybindings across devices using GitHub Gists",
	"version": "0.3.0",
	"qna": "marketplace",
	"publisher": "DunderDev",
	"icon": "./images/logo.png",
	"engines": {
		"vscode": "^1.90.0"
	},
	"repository": "https://github.com/0x3at/synceverything",
	"categories": [
		"Programming Languages",
		"Themes",
		"Keymaps",
		"Other",
		"Education"
	],
	"keywords": [
		"sync",
		"settings",
		"extensions",
		"keybindings",
		"gist",
		"profile",
		"syncing",
		"config"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "synceverything.showmenu",
				"title": "Show Menu",
				"category": "Sync Everything",
				"icon": "$(sync)"
			},
			{
				"command": "synceverything.createprofile",
				"title": "Create Profile",
				"category": "Sync Everything",
				"icon": "$(plus)"
			},
			{
				"command": "synceverything.pullprofile",
				"title": "Pull Profile",
				"category": "Sync Everything",
				"icon": "$(cloud-download)"
			},
			{
				"command": "synceverything.updateprofile",
				"title": "Update Profile",
				"category": "Sync Everything",
				"icon": "$(sync)"
			},
			{
				"command": "synceverything.deleteprofile",
				"title": "Delete Profile",
				"category": "Sync Everything",
				"icon": "$(trash)"
			},
			{
				"command": "synceverything.showlogs",
				"title": "Show Logs",
				"category": "Sync Everything",
				"icon": "$(output)"
			},
			{
				"command": "synceverything.setpathsmanually",
				"title": "Manually Set Paths",
				"category": "Sync Everything",
				"icon": "$(file-symlink-directory)"
			},
			{
				"command": "synceverything.showconfigview",
				"title": "Open Configuration View",
				"category": "Sync Everything",
				"icon": "$(window)"
			}
		],
		"configuration": {
			"title": "Sync Everything",
			"properties": {
				"synceverything.confirmBeforeSync": {
					"type": "boolean",
					"default": true,
					"description": "Show confirmation dialog before syncing"
				},
				"synceverything.showSyncMenuShortcut": {
					"type": "boolean",
					"default": true,
					"description": "Display a sync menu shortcut in the status bar"
				}
			}
		},
		"menus": {
			"commandPalette": [
				{
					"command": "synceverything.showmenu",
					"when": "true"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.10",
		"@types/node": "20.x",
		"@types/vscode": "^1.74.0",
		"@typescript-eslint/eslint-plugin": "^8.31.1",
		"@typescript-eslint/parser": "^8.31.1",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.5.2",
		"eslint": "^9.25.1",
		"ts-loader": "^9.5.2",
		"typescript": "^5.8.3",
		"webpack": "^5.99.7",
		"webpack-cli": "^6.0.1"
	},
	"dependencies": {
		"axios": "^1.9.0",
		"json5": "^2.2.3"
	}
}
